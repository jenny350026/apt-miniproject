{
    "manifest_version": 2,
    "background": { 
    "scripts": ["background.js"],
    "persistent": false
    },
    "description": "Add a context menu item to search Wiktionary.",
    "icons": {
      "16": "icon.png"
    },
    "browser_action": {
    "default_icon": "icon.png",
    "default_popup": "popup.html"
    },
    
    "minimum_chrome_version": "6",
    "name": "Connexus Right Click",
    "permissions": [ 
    "contextMenus", 
    "tabs" 
    ],
    "version": "1.0"
}